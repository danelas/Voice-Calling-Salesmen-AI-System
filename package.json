{
  "name": "voice-sales-ai",
  "version": "1.0.0",
  "description": "AI-powered voice calling salesmen system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "troubleshoot": "node scripts/troubleshoot.js",
    "quick-check": "node scripts/quick-check.js",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "logs:calls": "tail -f logs/calls.log"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "axios": "^1.6.0",
    "openai": "^4.20.1",
    "elevenlabs-node": "^1.3.0",
    "textmagic-rest-nodejs": "^2.0.1456",
    "@prisma/client": "^5.6.0",
    "prisma": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "voice",
    "sales",
    "calling",
    "automation",
    "elevenlabs",
    "openai",
    "textmagic"
  ],
  "author": "Your Name",
  "license": "MIT"
}
